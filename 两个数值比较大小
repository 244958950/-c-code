方法①
#include <stdio.h>
int Max(int x,int y)
{
    if(x>y)
    return x;
    else
    return y;
}
int main()
{
    int num1,num2,max;
    scanf("%d%d",&num1,&num2);
    max=Max(num1,num2);
    printf("max=%d\n",max);
    return 0;
}
方法②
#include<stdio.h>
int main()
{int a,b,max;
    scanf("%d%d",&a,&b);
    max=(a>b?a:b);
    printf("max=%d\n",max);
    return 0;
}
